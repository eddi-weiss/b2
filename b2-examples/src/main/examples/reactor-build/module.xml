<?xml version="1.0" encoding="UTF-8"?>
<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.sourcepit.b2.examples</groupId>
	<artifactId>build-reactor</artifactId>
	<version>${project.version}</version>
	<packaging>pom</packaging>

	<properties>
		<tycho.version>${tycho.version}</tycho.version>
		<b2.skipInterpolator>true</b2.skipInterpolator>
	</properties>

	<build>
		<extensions>
			<extension>
				<groupId>org.sourcepit.beef</groupId>
				<artifactId>b2-maven-plugin</artifactId>
				<version>${b2.extension.version}</version>
			</extension>
		</extensions>
	</build>

</project>
