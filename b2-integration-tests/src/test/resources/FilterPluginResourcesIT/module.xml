<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.sourcepit.b2.its</groupId>
	<artifactId>FilterPluginResourcesIT</artifactId>
	<version>1.0.0-SNAPSHOT</version>
	<packaging>pom</packaging>

	<properties>
		<project.vendor>Sourcepit.org</project.vendor>
		<project.execEnv>JavaSE-1.7</project.execEnv>
		<eclipse.runtimeVersion>[3.5,5)</eclipse.runtimeVersion>
		<tycho.version>${tycho.version}</tycho.version>
	</properties>

	<build>
		<plugins>
			<plugin>
				<groupId>org.sourcepit.b2</groupId>
				<artifactId>b2-maven-plugin</artifactId>
				<version>${project.version}</version>
				<extensions>true</extensions>
			</plugin>
		</plugins>
	</build>

</project>
